# Environment file for development
# Copy this to .env and customize for your setup

# Input directory - where video files are located
INPUT_DIR=/test_input

# Output directory - where audio files will be saved
OUTPUT_DIR=/test_output

# Output format: mp3 or flac
OUTPUT_FORMAT=mp3

# File naming
FILENAME_PREFIX=
FILENAME_POSTFIX=

# Processing configuration
MAX_PARALLEL_JOBS=4
STABILITY_PERIOD_SECONDS=30
MAX_RETRIES=3
RETRY_DELAY_SECONDS=60

# Logging
LOG_LEVEL=INFO

# Health server
HEALTH_PORT=8080

# Monitoring
POLLING_INTERVAL_SECONDS=10
